  function timeIn(data) {
      var totalTimeH = 0;
      var totalTimeM = 0;
     
    for (i = 0; i < data.length; i++) {
        var d1 = new Date(data[i].time_in);
        var d2 = new Date(data[i].time_out);
        var time = (d2 - d1)/1000;
        var ntime = new Date(null);
        ntime.setSeconds(time);
        ntime = ntime.toISOString().substr(11, 8);

        totalTimeH += (parseInt(ntime.substr(0,2)));
        totalTimeM += (parseInt(ntime.substr(3,2)));   
    }
    
    totalTimeH = parseInt(totalTimeH/data.length);
    totalTimeM = parseInt(totalTimeM/data.length);

    closeModal('createInput');
  
